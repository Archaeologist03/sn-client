{
  "name": "sn-react-redux-todo-app",
  "version": "2.0.1-alpha.0",
  "description": "Todo app with React and Redux built upon Sense/Net ECM",
  "main": "index.tsx",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "start": "react-scripts-ts start",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "check-coverage": "istanbul check-coverage ",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release pre && semantic-release post",
    "stop": "node server.stop.js",
    "fix": "npm run fix:prettier && npm run fix:tslint",
    "fix:prettier": "prettier \"{,!(dist|temp|bundle)/**/}*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "lint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SenseNet/sn-react-redux-todo-app.git"
  },
  "keywords": [
    "sensenet",
    "react",
    "redux"
  ],
  "author": "Sense/Net <snteam@sensenet.com> (http://sensenet.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-react-redux-todo-app.git/issues"
  },
  "homepage": "https://sensenet.com",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^1.1.1",
    "@sensenet/authentication-google": "^2.0.2-alpha.0",
    "@sensenet/client-core": "^1.3.2-alpha.0",
    "@sensenet/controls-react": "^2.3.6-alpha.0",
    "@sensenet/redux": "^5.1.3-alpha.0",
    "downshift": "^3.1.5",
    "keydown": "0.0.3",
    "material-ui-pickers": "^2.0.4",
    "radium": "^0.25.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-quill": "^1.3.3",
    "react-router-redux": "^4.0.8"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-dom": "^4.3.1",
    "autoprefixer": "^9.4.2",
    "chai": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "^3.1.0",
    "react-tap-event-plugin": "^3.0.3"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "types": [
    "jest",
    "node"
  ]
}
