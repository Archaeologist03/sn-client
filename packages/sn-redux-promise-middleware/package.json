{
  "name": "@sensenet/redux-promise-middleware",
  "version": "2.1.1-alpha.0",
  "description": "Redux middleware for async sensenet functions",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "npm run test",
    "commit": "git-cz",
    "pretest": "rimraf temp && tsc -p tsconfig.test.json",
    "test": "rimraf coverage && nyc mocha -p tsconfig.test.json ./temp/test/index.js",
    "fix": "npm run fix:prettier && npm run fix:tslint",
    "fix:prettier": "prettier \"{,!(dist|temp|bundle)/**/}*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "lint": "tslint --project .",
    "prebuild": "npm run lint",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "prepublishOnly": "npm run test && npm run build",
    "publish:development": "npm run build && npm t && npm run typedoc && npm publish --tag development",
    "typedoc": "typedoc --mode file --out documentation src --exclude **/index.ts --theme c:/Users/%USERNAME%/AppData/Roaming/npm/node_modules/@sensenet/typedoc-theme/sn-theme"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SenseNet/sn-client.git"
  },
  "keywords": [
    "sensenet",
    "ecm",
    "redux",
    "async",
    "middleware",
    "crud"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Sense/Net <snteam@sensenet.com> (http://sensenet.com/)",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-client/issues"
  },
  "nyc": {
    "cache": true,
    "include": "temp/src/**/*.*",
    "exclude": [
      "temp/test/**/*.*",
      "temp/src/Types.js",
      "**/index.js"
    ],
    "all": true,
    "reporter": [
      "lcov",
      "text-summary",
      "json"
    ]
  },
  "homepage": "https://sensenet.com",
  "config": {
    "commitizen": {
      "path": "sensenet-kfi-cz-conventional-changelog"
    }
  },
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "redux": "^4.0.1"
  }
}
